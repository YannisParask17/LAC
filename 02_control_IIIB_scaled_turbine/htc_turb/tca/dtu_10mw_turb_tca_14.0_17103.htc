    dll inpvec	2 1 # Mgen LSS [Nm];
    dll inpvec	2 2 # Pelec [W];	important for the assignment
    dll inpvec	2 3 # Mframe [Nm];
    dll inpvec	2 4 # Mgen HSS [Nm];
    dll inpvec	2 8 # Grid flag [0=run/1=stop];	
